{
  "name": "esports-kahoots",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "apps/middleware/*",
    "packages/*"
  ],
  "scripts": {
    "local": "turbo run local --parallel",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}