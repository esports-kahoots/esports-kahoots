{
    "compilerOptions": {
        "skipLibCheck": true,
        "useUnknownInCatchVariables": false,
        "module": "commonjs",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "outDir": "lib",
        "sourceMap": true,
        "strict": true,
        "target": "es2017",
        "noUnusedParameters": false
    },
    "compileOnSave": true,
    "include": ["src"]
}
